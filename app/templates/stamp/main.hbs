<div class="row">
    <div class="col-lg-3">
        <div class= "well well-lg" style="background-color: #FAFDFD">
            <p class="lead"><h3>Stamp</h3>
            <p><h5>Name: <b>{{name}}</b></h5>

            <div class="row">
                <div class="col-lg-12">
                    <h3 class="page-header"></h3>
                    {{#link-to 'stamp/search-stamp'}}
                        <button type="button" class="btn btn-primary btn-group-justified">
                            Go to search
                        </button>
                    {{/link-to}}
                </div>
            </div>
        </div>
    </div>
    <div class="col-lg-9">
        <ul class="nav nav-tabs " style="margin-bottom: 15px;">
            <li class="active"><a href="stampView" data-toggle="tab"><h3>Details</h3></a></li>
        </ul>

        <div class="tab-content">
            <div class="tab-pane fade active in" id="stampView">
                {{partial "stamp/partials/-details"}}
            </div>
        </div>
    </div>
</div>
