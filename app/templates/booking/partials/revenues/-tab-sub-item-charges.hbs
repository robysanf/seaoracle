<div class="row">
    <div class="col-lg-12">
        <table class="table table-condensed">
            <thead>
            <tr>
                <th style="width: 20%">Service</th>
                <th style="width: 30%">Bill to</th>
                <th style="width: 15%">Revenue</th>
                <th style="width: 15%">Cost</th>
                <th style="width: 10%">PER</th>
                <th style="width: 10%"></th>
            </tr>
            </thead>
            <tbody>
            <tr class="active">
                <td><strong>CONTAINER</strong></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
            </tr>
            {{#each chargeItem in chargeItems}}
                {{#if chargeItem.is_entityType_item}}
                    {{#if chargeItem.isTypeContainer}}
                        <tr>
                            <td>{{chargeItem.code}}</td>
                            <td>{{chargeItem.billTo.name}}</td>
                            <td>{{chargeItem.revenue}} {{chargeItem.revenueCurrency }}</td>
                            <td>{{chargeItem.cost}} {{chargeItem.costCurrency}}</td>
                            <td>{{chargeItem.multiplier}}</td>
                            {{#if chargeItem.isMultiplierNum}}
                                <td>{{chargeItem.num}}</td>
                            {{else}}
                                <td></td>
                            {{/if}}
                        </tr>
                    {{/if}}
                {{/if}}
            {{/each}}
                    <tr class="active">
                        <td><strong>RO/RO</strong></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                    </tr>
            {{#each chargeItem in chargeItems }}
                {{#if chargeItem.is_entityType_item}}
                    {{#if chargeItem.isTypeRoRo}}
                        <tr>
                            <td>{{chargeItem.code}}</td>
                            <td>{{chargeItem.billTo.name}}</td>
                            <td>{{chargeItem.revenue}} {{chargeItem.revenueCurrency }}</td>
                            <td>{{chargeItem.cost}} {{chargeItem.costCurrency}}</td>
                            <td>{{chargeItem.multiplier}}</td>
                            {{#if chargeItem.isMultiplierNum}}
                                <td>{{chargeItem.num}}</td>
                            {{else}}
                                <td></td>
                            {{/if}}
                        </tr>
                    {{/if}}
                {{/if}}
            {{/each}}
            <tr class="active">
                <td><strong>BB</strong></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
            </tr>
            {{#each chargeItem in chargeItems }}
                {{#if chargeItem.is_entityType_item}}
                    {{#if chargeItem.isTypeBB}}
                        <tr>
                            <td>{{chargeItem.code}}</td>
                            <td>{{chargeItem.billTo.name}}</td>
                            <td>{{chargeItem.revenue}} {{chargeItem.revenueCurrency }}</td>
                            <td>{{chargeItem.cost}} {{chargeItem.costCurrency}}</td>
                            <td>{{chargeItem.multiplier}}</td>
                            {{#if chargeItem.isMultiplierNum}}
                                <td>{{chargeItem.num}}</td>
                            {{else}}
                                <td></td>
                            {{/if}}
                        </tr>
                    {{/if}}
                {{/if}}
            {{/each}}
            </tbody>
        </table>
    </div>
</div>