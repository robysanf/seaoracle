<div id="login-layuot-container">
    <div id="layuot-container-title">
        <div><h2 style="color: #f5f5f5; padding-top: 15px; padding-left: 15px" ><strong>Sea Oracle</strong></h2></div>
    </div>
    <div id="login-layuot-inner">
        <div class="row" style="margin-top: 110px">
            <div class="col-lg-12">
                {{#if controller.newLog}}
                    {{partial "login/partials/-new"}}
                {{else}}
                    {{partial "login/partials/-submit"}}
                {{/if}}
            </div>
        </div>
    </div>
</div>

