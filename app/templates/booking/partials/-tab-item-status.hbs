<div class="row">
    <div class="col-lg-12">
        <div class="row" style="margin-top: 25px; margin-bottom: 25px">
            <div class="col-lg-12">
                <table class="table table-condensed">
                    <thead>
                    <tr style="border-bottom: 2px solid #000000;">
                        <th>Num.</th>
                        <th>Identifier</th>
                        <th>Description</th>
                        <th colspan="3">Status</th>
                        <th>Customs</th>
                        <th colspan="2">Delayed</th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr class="active">
                        <td><strong>CONTAINER</strong></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                    </tr>
                    {{#each item in items}}
                        {{#if item.tu_container}}
                            <tr>
                                <td >{{item.code}}</td>
                                {{#if item.freightEquipments}}
                                    {{#each freightEquipment in item.freightEquipments}}
                                        {{#if freightEquipment.equipment.code}}
                                            <td>{{freightEquipment.equipment.code}}</td>
                                        {{else}}
                                            <td>{{freightEquipment.equipment.equipmentClientCode}}</td>
                                        {{/if}}
                                    {{/each}}
                                {{else}}
                                    <td > </td>
                                {{/if}}
                                <td >{{item.classification}}</td>
                                {{#if item.freightEquipments}}
                                    {{#each freightEquipment in item.freightEquipments}}
                                        <td>
                                            {{#unless isRegister}}
                                                <a href="#" {{action "progressBar_removeState" freightEquipment item}}>
                                                    <span class="glyphicon glyphicon-chevron-left pull-right"></span>
                                                </a>
                                            {{/unless}}
                                        </td>
                                        <td >{{freightEquipment.currentStatus}}</td>
                                        <td>
                                            {{#unless isRegister}}
                                                <a href="#" {{action "progressBar_changeState" freightEquipment item}}>
                                                    <span class="glyphicon glyphicon-chevron-right pull-left"></span>
                                                </a>
                                            {{/unless}}
                                        </td>
                                    {{/each}}
                                {{else}}
                                    <td > </td>
                                    <td > </td>
                                    <td > </td>
                                {{/if}}
                                <td align="center"> {{input type="checkbox" name="custom" checked=item.custom disabled=isRegister}}</td>
                                <td>From:</td>
                                <td>{{item.bookingFrom.code}}</td>
                            </tr>
                        {{/if}}
                    {{/each}}

                            <tr class="active">
                                <td><strong>RO/RO</strong></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                            </tr>
                    {{#each item in items}}
                        {{#if item.tu_roro}}
                            <tr>
                                <td >{{item.code}}</td>
                                <td >{{item.chassisNum}} </td>
                                <td >{{item.classification}}</td>
                                {{#if item.freightEquipments}}
                                    {{#each freightEquipment in item.freightEquipments}}
                                        <td>
                                            {{#unless isRegister}}
                                                <a href="#" {{action "progressBar_removeState" freightEquipment item}}><span class="glyphicon glyphicon-chevron-left pull-right"></span></a>
                                            {{/unless}}
                                        </td>
                                        <td >{{freightEquipment.currentStatus}}</td>
                                        <td>
                                            {{#unless isRegister}}
                                                <a href="#" {{action "progressBar_changeState" freightEquipment item}}><span class="glyphicon glyphicon-chevron-right pull-left"></span></a>
                                            {{/unless}}
                                        </td>
                                    {{/each}}
                                {{else}}
                                    <td > </td>
                                    <td > </td>
                                    <td > </td>
                                {{/if}}
                                <td align="center"> {{input type="checkbox" name="custom" checked=item.custom disabled=isRegister}}</td>
                                <td>From:</td>
                                <td>{{item.bookingFrom.code}}</td>
                            </tr>
                        {{/if}}
                    {{/each}}

                            <tr class="active">
                                <td><strong>BB</strong></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                            </tr>
                    {{#each item in items}}
                        {{#if item.tu_bb}}
                            <tr>
                                <td >{{item.code}}</td>
                                <td > </td>
                                <td >{{item.classification}}</td>
                                {{#if item.freightEquipments}}
                                    {{#each freightEquipment in item.freightEquipments}}
                                        <td>
                                            {{#unless isRegister}}
                                                <a href="#" {{action "progressBar_removeState" freightEquipment item}}><span class="glyphicon glyphicon-chevron-left pull-right"></span></a>
                                            {{/unless}}
                                        </td>
                                        <td >{{freightEquipment.currentStatus}}</td>
                                        <td>
                                            {{#unless isRegister}}
                                                <a href="#" {{action "progressBar_changeState" freightEquipment item}}><span class="glyphicon glyphicon-chevron-right pull-left"></span></a>
                                            {{/unless}}
                                        </td>
                                    {{/each}}
                                {{else}}
                                    <td > </td>
                                    <td > </td>
                                    <td > </td>
                                {{/if}}
                                <td align="center">{{input type="checkbox" name="custom" checked=item.custom disabled=isRegister}}</td>
                                <td>From:</td>
                                <td>{{item.bookingFrom.code}}</td>
                            </tr>
                        {{/if}}
                    {{/each}}
                    </tbody>
                </table>
                {{#unless isRegister}}
                    <div class="row">
                        <h5 class="page-header"></h5>
                        <div class="col-lg-12">
                            <button type="button" style="text-align: left" class="btn btn-primary pull-right" {{action 'updateAll' this}}>
                                <span class="glyphicon glyphlink glyphicon-ok"  style="color: aliceblue"></span>Update customs
                            </button>
                        </div>
                    </div>
                {{/unless}}
            </div>
        </div>
    </div>
</div>
