<div class="modal-header">
    <button type="button" class="close" {{action close 'overview' 'application' target="view"}}>&times;</button>
    <h3>Warehouse details</h3>
</div>
<div class="modal-body">
    <div class="row">
        <div class="col-lg-3"><h4>Name</h4></div>
        <div class="col-lg-7">
            <div class="form-group">{{input value= controller.poiRecord.name class='form-control' disabled="disabled"}}</div>
        </div>
        <div class="col-lg-2"></div>
    </div>
    <div class="row">
        <div class="col-lg-3"><h4>Company Owner</h4></div>
        <div class="col-lg-7">
            <div class="form-group">{{input value= controller.poiRecord.company.name class='form-control' disabled="disabled"}}</div>
        </div>
        <div class="col-lg-2"></div>
    </div>
    <div class="row">
        <div class="col-lg-3"><h4>Street</h4></div>
        <div class="col-lg-7">
            <div class="form-group">{{input value= controller.poiRecord.street class='form-control' disabled="disabled"}}</div>
        </div>
        <div class="col-lg-2"></div>
    </div>
    <div class="row">
        <div class="col-lg-3"><h4>City</h4></div>
        <div class="col-lg-7">
            <div class="form-group">{{input value= controller.poiRecord.city class='form-control' disabled="disabled"}}</div>
        </div>
        <div class="col-lg-2"></div>
    </div>
    <div class="row">
        <div class="col-lg-3"><h4>Country</h4></div>
        <div class="col-lg-7">
            <div class="form-group">{{input value= controller.poiRecord.country class='form-control' disabled="disabled"}}</div>
        </div>
        <div class="col-lg-2"></div>
    </div>
    <div class="row">
        <div class="col-lg-3"><h4>Notify Party</h4></div>
        <div class="col-lg-7">
            <div class="form-group">{{input value= controller.poiRecord.notifyParty class='form-control' disabled="disabled"}}</div>
        </div>
        <div class="col-lg-2"></div>
    </div>
    <div class="row">
        <div class="col-lg-3"><h4>Phone</h4></div>
        <div class="col-lg-7">
            <div class="form-group">{{input value= controller.poiRecord.phone class='form-control' disabled="disabled"}}</div>
        </div>
        <div class="col-lg-2"></div>
    </div>
</div>
<div class="modal-footer">
    <a href="#" class="btn" {{action close 'overview' 'application' target="view"}}>Close</a>
</div>