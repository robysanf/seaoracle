<div class="container">
    <div class="row" style="margin-top: 70px">
        <div class="col-lg-12">
            <div class="row">
                <div class="col-lg-1"></div>
                <div class="col-lg-10">
                    <div class= "well well-lg" style="background-color: #FAFDFD">
                        <div class="row">
                            <div class="col-lg-6">
                                <p class="lead">Equipment classification Search</p>
                            </div>
                            <div class="col-lg-6">
                                {{#view "search"}}
                                    <button type="button" class="btn btn-primary btn-130 pull-right">
                                        <span class="glyphicon glyphbutton glyphicon-search"></span> Search
                                    </button>
                                {{/view}}
                            </div>
                        </div>
                        <div class="row" style="margin-top: 25px; margin-bottom: 15px">
                            <div class="col-lg-1"></div>
                            <div class="col-lg-5">
                                <label>Name</label>
                                {{input value= controller.searchName class='form-control'}}
                            </div>
                            <div class="col-lg-5">
                                <label>Size</label>
                                <!--<a id="autocomplete-searchSizeCode">-->
                                {{select-2
                                content= app_controller.autocompleteEqClassification
                                value= controller.searchSizeCode
                                optionLabelPath="sizeCode"
                                placeholder="Choose a size"
                                typeaheadNoMatchesText="No size found for '%@'"
                                typeaheadErrorText="Loading failed: %@"
                                enabled=true
                                allowClear=true}}
                                    {{!{{auto-complete source=app_controller.autocompleteEqClassification destination=controller.searchSizeCode searchPath="sizeCode" minChars=1}}
                                <!--</a>-->
                            </div>
                            <div class="col-lg-1"></div>
                        </div>
                        <div class="row" style="margin-bottom: 15px">
                            <div class="col-lg-1"></div>
                            <div class="col-lg-5">
                                <label>Type</label>
                                <!--<a id="autocomplete-searchTypeCode">-->
                                {{select-2
                                content= app_controller.autocompleteEqClassification
                                value= controller.searchTypeCode
                                optionLabelPath="typeCode"
                                placeholder="Choose a type"
                                typeaheadNoMatchesText="No type found for '%@'"
                                typeaheadErrorText="Loading failed: %@"
                                enabled=true
                                allowClear=true}}
                                    {{!{{auto-complete source=app_controller.autocompleteEqClassification destination=controller.searchTypeCode searchPath="typeCode" minChars=1}}
                                <!--</a>-->
                            </div>
                            <div class="col-lg-5">
                                <label>ISO code</label>
                                <!--<a id="autocomplete-searchIsoCode">-->
                                {{select-2
                                content= app_controller.autocompleteEqClassification
                                value= controller.searchIsoCode
                                optionLabelPath="isoCode"
                                placeholder="Choose a iso code"
                                typeaheadNoMatchesText="No iso code found for '%@'"
                                typeaheadErrorText="Loading failed: %@"
                                enabled=true
                                allowClear=true}}
                                    {{!{{auto-complete source=app_controller.autocompleteEqClassification destination=controller.searchIsoCode searchPath="isoCode" minChars=1}}
                                <!--</a>-->
                            </div>
                            <div class="col-lg-1"></div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-1"></div>
            </div>
        </div>
    </div>
</div>
